<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="pane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <Button mnemonicParsing="false" onAction="#search" text="Search" />
            <TextField fx:id="searchString" />
        </items>
      </ToolBar>
   </top>
    <center>
        <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </center>
   <bottom>
      <ButtonBar buttonMinWidth="60.0" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
            <Button fx:id="largePrev" disable="true" mnemonicParsing="false" onAction="#doPageButton" text="&lt;&lt;" />
            <Button fx:id="prev" disable="true" mnemonicParsing="false" onAction="#doPageButton" text="&lt;" />
            <Label fx:id="pageNumber" alignment="CENTER" minWidth="0.0" prefHeight="16.0" />
            <Button fx:id="next" mnemonicParsing="false" onAction="#doPageButton" text="&gt;" />
            <Button fx:id="largeNext" mnemonicParsing="false" onAction="#doPageButton" text="&gt;&gt;" />
        </buttons>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </ButtonBar>
   </bottom>
</BorderPane>
